{
  "name": "PTC_Portal",
  "version": "1.0.0",
  "description": "Portal is ideal place to track students progress",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "start-dev": "NODE_ENV=development babel-node ./server/server.js",
    "build-client": "NODE_ENV=production webpack --config ./webpack.production.config.js --profile",
    "build-server": "babel -d ./dist ./server -s",
    "lint": "eslint src; exit 0",
    "clean": "rimraf dist",
    "postinstall": "npm run clean && npm run build-client && npm run build-server",
    "test": "mocha --compilers js:babel-core/register --require ignore-styles --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "engines": {
    "node": "5.7.0",
    "npm": "3.6.0"
  },
  "repository": {
